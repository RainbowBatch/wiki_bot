{% import 'macros.template' as macro %}

{% raw %}{{{% endraw %}DEFAULTSORT:{{episode_details.transcript_sortkey}}{% raw %}}}{% endraw %}
{%- if episode_details.transcript_safe_clean_title != episode_details.transcript_clean_title -%}{%- raw -%}{{{%- endraw -%}DISPLAYTITLE:{{episode_details.transcript_clean_title}}{%- raw -%}}}{%- endraw -%}{%- endif -%}
{% raw %}{{Transcript{% endraw %}
    | title           = {{ macro.wiki_link(episode_details.title, episode_details.safe_title) }}
    | caption         = [{{episode_details.libsyn_page}} Listen to this episode on Libsyn]
    | episodeNumber   = {{ macro.wiki_link(episode_details.episode_number, episode_details.safe_title) }}
    | length          = {{episode_details.episode_length}}
{% raw %}}}{% endraw %}

{% raw %}{{MessageBox{% endraw %}
    | header     = Warning: Bot Generated Content
    | text       = This transcript was automatically generated by transcription software and likely contains many mistakes and misattributions. Please check the audio for definitive quotes, attribution, and context.
    | image      = rainbowbatch_avatar.png
    | imagewidth = 50px
    | id         = warning
{% raw %}}}{% endraw %}

{% for transcript_block in transcript_blocks %}
{% raw %}{{TranscriptBlock{% endraw %}
    | name      = {{transcript_block.speaker_name | format_speaker}}
    | timestamp = {{transcript_block.start_timestamp | format_timestamp}}
    | text      = {{transcript_block.text}}
{% raw %}}}{% endraw %}
{% endfor %}

